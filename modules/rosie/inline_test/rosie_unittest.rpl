
import id
import word

alias wb = [:space:]+
pat = id.id1
slocal = "local"   -- local is a reserved word in RPL
accept = "accepts"
reject = "rejects"
include = "includes" pat
exclude = "excludes" pat
input = word.q

unittest = { "--" wb "test" wb {slocal wb}? pat wb {accept / reject / include / exclude} wb input {wb* "," wb* input}*
